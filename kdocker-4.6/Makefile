	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/kdocker/icons/kdocker.png
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/kdocker/icons/ 


install_desktop: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/applications/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/applications/ 
	-$(INSTALL_FILE) /home/katharine/Sources/kdocker/kdocker-4.6/helpers/kdocker.desktop $(INSTALL_ROOT)/usr/share/applications/


uninstall_desktop:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/applications/kdocker.desktop
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/applications/ 


install_completion: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/etc/bash_completion.d/ || $(MKDIR) $(INSTALL_ROOT)/etc/bash_completion.d/ 
	-$(INSTALL_FILE) /home/katharine/Sources/kdocker/kdocker-4.6/helpers/kdocker $(INSTALL_ROOT)/etc/bash_completion.d/


